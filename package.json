{
  "name": "wedding-launi-me",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "LOCAL_DB_PATH=$(find .wrangler/state/v3/d1/miniflare-D1DatabaseObject -type f -name '*.sqlite' -print -quit) astro dev",
    "build": "astro check && astro build",
    "preview": "astro build && wrangler pages dev ./dist --d1 WEDDING_DB=1b6c65fc-3b2d-40d4-afb2-afa5943ba801",
    "astro": "astro",
    "cf-typegen": "wrangler types",
    "db:generate": "drizzle-kit generate",
    "db:migrate:local": "wrangler d1 migrations apply --env preview WEDDING_DB --local",
    "db:migrate:preview": "wrangler d1 migrations apply --env preview WEDDING_DB --remote",
    "db:migrate:production": "wrangler d1 migrations apply --env production WEDDING_DB --remote",
    "db:studio:local": "LOCAL_DB_PATH=$(find .wrangler/state/v3/d1/miniflare-D1DatabaseObject -type f -name '*.sqlite' -print -quit) drizzle-kit studio",
    "db:studio:preview": "source .drizzle.env && DB_ID='1b6c65fc-3b2d-40d4-afb2-afa5943ba801' drizzle-kit studio",
    "db:studio:production": "source .drizzle.env && DB_ID='77b33bc0-b9a1-47f3-a7a9-2117d3eeba7c' drizzle-kit studio"
  },
  "dependencies": {
    "@astrojs/cloudflare": "^11.0.4",
    "@astrojs/react": "^3.6.2",
    "@astrojs/tailwind": "^5.1.0",
    "@lucia-auth/adapter-sqlite": "^3.0.2",
    "@nanostores/react": "^0.7.3",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@uploadthing/react": "^6.7.2",
    "astro": "^4.13.3",
    "clsx": "^2.1.1",
    "drizzle-orm": "^0.33.0",
    "input-otp": "^1.2.4",
    "lucia": "^3.2.0",
    "nanostores": "^0.11.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tailwind-merge": "^2.5.2",
    "tailwindcss": "^3.4.10",
    "uploadthing": "^6.13.2",
    "yet-another-react-lightbox": "^3.21.4"
  },
  "devDependencies": {
    "typescript": "^5.5.4",
    "@astrojs/check": "^0.9.2",
    "@cloudflare/workers-types": "^4.20240806.0",
    "@types/node": "^22.4.1",
    "better-sqlite3": "^11.1.2",
    "cross-env": "^7.0.3",
    "drizzle-kit": "^0.24.0",
    "wrangler": "^3.72.2"
  }
}